#include <iostream>
#include <string>
using namespace std;
int main()
{
	int n;
	cin >> n;
	string* s = new string[n];
	for (int i = 0; i < n; i++)
		cin >> s[i];
	for (int i = 0; i < n; i++)
	{	
		if (s[i][0] + s[i][1] + s[i][2] == s[i][3] + s[i][4] + s[i][5])
			cout << "YES\n";
		else
			cout << "NO\n";
	}
}
